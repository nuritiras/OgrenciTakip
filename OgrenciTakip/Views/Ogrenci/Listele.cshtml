@{
}
@model List<OgrenciModel>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Okul No</th>
            <th scope="col">İsim</th>
            <th scope="col">Soyisim</th>
            <th scope="col">Sınıfı</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var talebe in Model)
        {
            <tr>
                <th scope="row">@talebe.OkulNo</th>
                <td>@talebe.Isim </td>
                <td>@talebe.SoyIsim</td>
                <td>@talebe.Sinifi</td>
            </tr>
        }
    </tbody>
</table>